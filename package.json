{
  "name": "create-fastify-typescript-api",
  "version": "1.0.0",
  "description": "Fastify TypeScript API模板，包含JWT认证、PostgreSQL数据库、Swagger文档",
  "keywords": ["fastify", "typescript", "jwt", "postgresql", "api", "template", "create"],
  "author": {
    "name": "Developer",
    "email": "developer@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishkiller111/create-fastify-typescript-api.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/create-fastify-typescript-api/issues"
  },
  "homepage": "https://github.com/your-username/create-fastify-typescript-api#readme",
  "bin": {
    "create-fastify-api": "./setup.js"
  },
  "files": [
    "src/**/*",
    "scripts/**/*",
    "tsconfig.json",
    "template.json",
    ".env.template",
    "setup.js",
    "CLAUDE.md",
    "IFLOW.md"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec 'node --loader ts-node/esm' src/server.ts",
    "migrate": "node dist/migrations/run-migrations.js",
    "setup": "node setup.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.8.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "ts-node": "^10.9.2"
  },
  "type": "module"
}